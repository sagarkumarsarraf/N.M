y = []
n = int(input("Number of element in array :"))
print("Enter the element of x:")
for i in range(0, n):
  l = float(input())
  x.append(l)
print("Enter the element of y:")
for i in range(0, n):
  m = float(input())
  y.append(m)

print("Element of x=", x)
print("Element of y=", y)
h = x[1] - x[0]
print(h)
s = 0
for i in range(1, n - 1):
  s = s + y[i]
print("s=", s)
A = (h / 2) * (y[0] + y[4] + (2 * s))
print("the required value is ", A)
